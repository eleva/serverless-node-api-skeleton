model notification {
  id                 String   @id @db.Char(36)
  user_id            Int?
  notification_class String   @db.VarChar(255)
  notification_html  String   @db.VarChar(255)
  resource_id        Int
  create_time        DateTime @db.DateTime(0)
  account            account? @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_BF5476CAA76ED395")

  @@index([user_id], map: "IDX_BF5476CAA76ED395")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model reset_password_request {
  id           Int      @id @default(autoincrement())
  user_id      Int
  selector     String   @db.VarChar(20)
  hashed_token String   @db.VarChar(100)
  requested_at DateTime @db.DateTime(0)
  expires_at   DateTime @db.DateTime(0)
  account      account  @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_7CE748AA76ED395")

  @@index([user_id], map: "IDX_7CE748AA76ED395")
}

model soil_quality_range {
  id          Int      @id @default(autoincrement())
  parameter   String   @db.VarChar(50)
  value_min   Float    @default(0)
  value_max   Float    @default(0)
  unit        String?  @db.VarChar(20)
  create_time DateTime @db.DateTime(0)
  edit_time   DateTime @db.DateTime(0)
  create_user Int      @default(0)
  edit_user   Int      @default(0)
  del         Int      @default(0)
}

model system_language {
  id          Int      @id @default(autoincrement())
  code        String   @db.VarChar(2)
  name        String   @db.VarChar(255)
  create_time DateTime @db.DateTime(0)
  edit_time   DateTime @db.DateTime(0)
  create_user Int      @default(0)
  edit_user   Int      @default(0)
  del         Int      @default(0)
  active      Boolean  @default(true)
}

model system_settings {
  id                              Int      @id @default(autoincrement())
  system_email_sender             String   @db.VarChar(255)
  system_email_sender_name        String?  @db.VarChar(150)
  system_bcc_email                String?  @db.VarChar(255)
  default_language                String?  @db.VarChar(2)
  current_version                 String?  @db.VarChar(10)
  email_delivery_whitelist_status Int?     @default(0)
  email_delivery_whitelist        String?  @db.VarChar(255)
  upsert_last_filters             Boolean  @default(false)
  public_area                     Boolean? @default(false)
  company_name                    String?  @db.VarChar(200)
  contact_email                   String?  @db.VarChar(200)
  website                         String?  @db.VarChar(200)
  additional_info                 String?  @db.VarChar(200)
  logo                            String?  @db.VarChar(255)
  work_in_progress_enabled        Boolean? @default(true)
  work_in_progress_login_enabled  Boolean? @default(true)
  work_in_progress_title          String?  @db.VarChar(255)
  work_in_progress_subtitle       String?  @db.VarChar(255)
  work_in_progress_background     String?  @db.VarChar(255)
}

model translation {
  id          Int    @id @default(autoincrement())
  domain      String @db.VarChar(255)
  locale      String @db.VarChar(2)
  trans_key   String @db.VarChar(255)
  translation String @db.LongText
}

model user_data_table_filters {
  id                  Int      @id @default(autoincrement())
  user_id             Int
  type                String   @db.VarChar(100)
  route               String   @db.VarChar(255)
  filters             String?  @db.LongText
  insert_timestamp    DateTime @map("_insert_timestamp") @db.DateTime(0)
  last_edit_timestamp DateTime @map("_last_edit_timestamp") @db.DateTime(0)
}

model weather_station {
  id                 Int                  @id @default(autoincrement())
  latitude           Float
  longitude          Float
  create_time        DateTime             @db.DateTime(0)
  edit_time          DateTime             @db.DateTime(0)
  create_user        Int                  @default(0)
  edit_user          Int                  @default(0)
  del                Int                  @default(0)
  historical_weather historical_weather[]
  land               land[]
}
